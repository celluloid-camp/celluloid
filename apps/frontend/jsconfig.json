{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~actions/*": [
                "src/actions/*"
            ],
            "~components/*": [
                "src/components/*"
            ],
            "~hooks/*": [
                "src/hooks/*"
            ],
            "~images/*": [
                "src/images/*"
            ],
            "~locales/*": [
                "src/locales/*"
            ],
            "~pages/*": [
                "src/pages/*"
            ],
            "~reducers/*": [
                "src/reducers/*"
            ],
            "~services/*": [
                "src/services/*"
            ],
            "~store/*": [
                "src/store/*"
            ],
            "~theme/*": [
                "src/theme/*"
            ],
            "~types/*": [
                "src/types/*"
            ],
            "~utils/*": [
                "src/utils/*"
            ],
            "~/*": [
                "src/*"
            ],
            "~login/*": [
                "src/components/login/*"
            ]
        }
    }
}