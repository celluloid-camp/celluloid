{
  "name": "@celluloid/types",
  "version": "3.3.0",
  "private": true,
  "description": "Common types library for Celluloid client and server",
  "repository": "http://github.com/celluloid-edu/celluloid",
  "author": "Younes Benaomar <younes.benaomar@gmail.com>",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "pkgroll build",
    "clean": "git clean -xdf .cache .turbo dist node_modules"
  },
  "devDependencies": {
    "@celluloid/config": "workspace:*",
    "pkgroll": "^2.11.2",
    "typescript": "catalog:"
  },
  "pkgroll": {
    "input": "src/index.ts",
    "output": "dist",
    "format": "esm",
    "minify": true,
    "sourcemap": true
  }
}
